<div class="container">
    <div class="text-center m-3 p-2"><strong>Bikes located in <%= @query%></strong></div>
  <div class="row m-3">
    <% @bicycles.each do |bicycle|  %>
    <%= render "card", bicycle: bicycle %>
    <% end %>
  </div>
</div>
<div
  id="map"
  style="width: 100%;
  height: 600px;"
  data-markers="<%= @markers.to_json %>"
  data-mapbox-api-key="<%= ENV['MAPBOX_API_KEY'] %>"
></div>
